<div id="card-climb-popup">
  <div class="card-body">
    <h5 class="card-title"><%= link_to climb.name, climb_path(climb) %></h5>
    <h6 class="card-subtitle mb-2 text-muted"><%= climb.grade %> | <%= climb.approach %></h6>
    <h6><%= climb.region %></h6>
    <p class="card-text">Fantastic climb with great views.</p>
    <%= link_to "Trip reports", climb_path(climb) %>
  </div>
</div>
